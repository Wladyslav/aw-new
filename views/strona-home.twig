<section class="home_tabs">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h2>
                    {{o.home_tabs_h1}}
                </h2>
                <nav>
                    <div class="nav nav-tabs row" id="nav-tab" role="tablist">
                        <button class="active col-12 col-lg-4 d-flex justify-lg-content-around nav-link" id="nav-1-tab" data-bs-toggle="tab" data-bs-target="#nav-1" type="button" role="tab" aria-controls="nav-1" aria-selected="true">
                            <span class="home_tabs--digit">01</span>
                            <div class="mx-sm-auto">
                                <span class="home_tabs--title d-block">{{o.home_tabs_1_title}}</span>
                                <p>{{o.home_tabs_1_desc}}</p>
                            </div>
                        </button>
                        <button class="col-12 col-lg-4 d-flex justify-lg-content-around nav-link" id="nav-2-tab" data-bs-toggle="tab" data-bs-target="#nav-2" type="button" role="tab" aria-controls="nav-2" aria-selected="true">
                            <span class="home_tabs--digit">02</span>
                            <div class="mx-sm-auto">
                                <span class="home_tabs--title d-block">{{o.home_tabs_2_title}}</span>
                                <p>{{o.home_tabs_2_desc}}</p>
                            </div>
                        </button>
                        <button class="col-12 col-lg-4 d-flex justify-lg-content-around nav-link" id="nav-3-tab" data-bs-toggle="tab" data-bs-target="#nav-3" type="button" role="tab" aria-controls="nav-3" aria-selected="true">
                            <span class="home_tabs--digit">03</span>
                            <div class="mx-sm-auto">
                                <span class="home_tabs--title d-block">{{o.home_tabs_3_title}}</span>
                                <p>{{o.home_tabs_3_desc}}</p>
                            </div>
                        </button>
                    </div>
                </nav>
                <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane fade show active" id="nav-1" role="tabpanel" aria-labelledby="nav-1-tab" tabindex="0">
                        <div class="row">
                            <div class="col-12 col-lg-6 order-1 order-lg-0">
                                <div class="left-box">
                                    <h1>{{o.home_tabs_1_c_title}}</h1>
                                    <h4 class="mt-3">{{o.home_tabs_1_c_desc}}</h4>
                                    <button class="btn no-border arrow ps-0 text-red mt-5">
                                        <a href="{{o.home_tabs_1_button_link}}">{{o.home_tabs_1_button_title}}</a>
                                    </button>
                                </div>

                            </div>
                            <div class="col-12 col-lg-6 mb-5 mb-lg-0">
                                <div class="img_bg position-relative">
                                    <img src="{{o.home_tabs_1_c_img.url}}" class="img-fluid w-100" alt="{{o.home_tabs_1_c_img.alt}}" />
                                    <div class="image__circle"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="nav-2" role="tabpanel" aria-labelledby="nav-2-tab" tabindex="0">
                        <div class="row">
                            <div class="col-12 col-lg-6">
                                <div class="left-box">
                                    <h1>{{o.home_tabs_2_c_title}}</h1>
                                    <h4 class="mt-3">{{o.home_tabs_2_c_desc}}</h4>
                                    <button class="btn no-border arrow ps-0 text-red mt-5">
                                        <a href="{{o.home_tabs_2_button_link}}">{{o.home_tabs_2_button_title}}</a>
                                    </button>
                                </div>

                            </div>
                            <div class="col-12 col-lg-6">
                                <div class="img_bg position-relative">
                                    <img src="{{o.home_tabs_2_c_img.url}}" class="img-fluid w-100" alt="{{o.home_tabs_2_c_img.alt}}" />
                                    <div class="image__circle"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="nav-3" role="tabpanel" aria-labelledby="nav-3-tab" tabindex="0">
                        <div class="row">
                            <div class="col-12 col-lg-6">
                                <div class="left-box">
                                    <h1>{{o.home_tabs_3_c_title}}</h1>
                                    <h4 class="mt-3">{{o.home_tabs_3_c_desc}}</h4>
                                    <button class="btn no-border arrow ps-0 text-red mt-5">
                                        <a href="{{o.home_tabs_3_button_link}}">{{o.home_tabs_3_button_title}}</a>
                                    </button>
                                </div>

                            </div>
                            <div class="col-12 col-lg-6">
                                <div class="img_bg position-relative">
                                    <img src="{{o.home_tabs_3_c_img.url}}" class="img-fluid w-100" alt="{{o.home_tabs_3_c_img.alt}}" />
                                    <div class="image__circle"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-12 col-xl-8 mx-auto">
                <div class="home_tabs__newsletter">
                    <h3 class="text-center mb-3">{{o.home_tabs__newsletter_h1}}</h3>
                    <div class="text-center">
                        <input type="text" placeholder="{{o.home_tabs__newsletter_input}}" />
                        <button class="btn btn-red ms-3 mt-2 mt-lg-0">{{o.home_tabs__newsletter_button}}</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="statistics">
    <div class="container">
        <div class="row g-0">
            <div class="col-12 col-lg-5">
                <h2>
                    {{o.statistics_h1}}
                </h2>

                <p class="mt-5">
                    {{o.statistics_content_left}}
                </p>

                <div class="d-inline-flex mb-3 mb-md-0 mt-4 mt-md-5 offset-0 flex-column flex-md-row offset-md-2 offset-lg-0 statistics__switch">
                    <div class="before">{{o.statistics_before}}</div>
                    <div class="statistics__switch--area my-auto">
                        <input type="checkbox" class="multi-switch" value="0">
                    </div>
                    <div class="after active">{{o.statistics_after}}</div>
                </div>
            </div>

            <div class="col-12 col-lg-7">
                <div class="row statistics__right position-relative">
                    <div class="col-12 col-lg-10 my-3 my-lg-0 offset-lg-2">
                        <p>{{o.statistics_content_right}}</p>
                        <img src="{{o.statistics_image.url}}" alt="{{o.statistics_image.alt}}" class="stat_bg img-fluid my-4" />
                    </div>

                    <div class="col-12 statistics__outer">
                        <div class="statistics__icons">
                            <div class="row">
                                {% for s in o.statistics_stats %}
                                <div class="col-4 col-sm-4 d-inline-flex flex-column flex-sm-row justify-content-evenly">
                                    <div class="statistics__icons--icon d-inline">
                                        <img src="/assets/img/icon_s_{{loop.index}}.svg" alt="icon 1" />
                                    </div>
                                    <div class="statistics__icons--text">
                                        <span class="digit d-block" d-before="{{s.digit|replace({' ': ''})}}" d-after="{{s.digit_after|replace({' ': ''})}}">{{s.digit}}</span>
                                        <span>{{s.desc}}</span>
                                    </div>
                                </div>
                                {% endfor %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="consultation">
    <div class="consultation__bg bg_1"></div>
    <div class="consultation__bg bg_2"></div>
    <div class="consultation__bg bg_3"></div>
    <div class="consultation__bg bg_4"></div>
    <div class="consultation__bg bg_5"></div>
    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-5 g-md-0">
                <h2>
                    {{o.consultation_h1}}
                </h2>
            </div>

            <div class="col-12 col-lg-7">
                <div class="row consultation__right position-relative">
                    <div class="col-12 col-lg-10 g-md-0 offset-0 offset-lg-2">
                        <p>{{o.consultation_desc}}
                        </p>
                    </div>
                </div>
            </div>
        </div>

        <div class="consultation-items row align-items-stretch">
            {% for c in consultations %}
            <div class="col-12 col-lg-6 accordion">
                <div class="consultation-item">
                    <div class="icon down"></div>
                    <div class="row">
                        <div class="col-12 col-md-10 col-lg-9 order-2 order-md-1">
                            <div class="consultation-item__left">
                                <h3>{{c.title}}</h3>
                                <p>{{c.desc}}</p>
                            </div>
                        </div>

                        <div class="col-12 col-md-2 col-lg-3 d-flex align-items-center justify-content-md-end order-1 order-md-2">
                            <img src="{{c.meta('icon').url}}" class="img-fluid" alt="{{c.icon.alt}}">
                        </div>
                        <div class="consultation-item__end col-12 order-3 order-md-3">
                            <div class="consultation-item__button">
                                <button class="btn no-border arrow ps-0">
                                    <a href="{{c.button_link}}">{{c.button}}</a>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<section class="banner_newsletter">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-12 col-xl-9 position-relative">
                <div class="banner_newsletter__box mx-auto">
                    <h2>{{o.banner_newsletter_h1}}</h2>
                    <p>{{o.banner_newsletter_desc}}</p>
                    <input class="d-block w-100 mt-4" type="text" placeholder="{{o.banner_newsletter_input}}" />
                    <button class="btn btn-red d-block mx-auto">{{o.banner_newsletter_button}}</button>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="partners">
    <div class="container">
        <div class="row g-0">
            <div class="col-12 col-lg-5">
                <h2>
                    {{o.our_partners_h1}}
                </h2>

                <p class="mt-5 d-none d-lg-block d-xl-block">
                    {{o.our_partners_left_desc}}
                </p>

                <button class="btn btn-border-only d-none d-lg-block d-xl-block mt-5 mx-auto mx-lg-0">
                    <a href="{{o.our_partners_button_link}}">{{o.our_partners_button}}</a>
                </button>
            </div>

            <div class="col-12 col-lg-7">
                <div class="row partners__right position-relative justify-content-end">
                    <div class="g-lg-0 col-12 col-lg-10 offset-lg-2">
                        <p class="first">{{o.our_partners_right_desc}}</p>
                    </div>

                    <p class="mt-3 mt-lg-5 d-lg-none d-xl-none">
                        {{o.our_partners_left_desc}}
                    </p>

                    <div class="col-12 col-lg-10 g-lg-0 offset-lg-2">
                        <div class="row justify-content-center">
                            {% for p in o.our_partners_logotypes %}
                            <div class="col-4 col-lg-3 my-auto text-center">
                                <img src="{{p.logo.url}}" class="img-fluid" alt="{{p.logo.alt}}" />
                            </div>
                            {% endfor %}
                        </div>
                    </div>

                    <button class="btn d-lg-none d-xl-none mt-5 mx-auto mx-lg-0">{{o.our_partners_button}}</button>
