</section>

<section class="opinions">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h2>
                    {{o.our_opinions_h1}}
                </h2>
                <p class="d-block mx-auto text-center">{{o.our_opinions_desc}}</p>
            </div>
        </div>

        <div class="opinions_items">
            <div class="justify-content-evenly row">
                {% for op in opinions %}
                <div class="col-12 col-lg-5 mb-5">
                    <div class="opinions_item">
                        <div class="opinions_item__icon"></div>
                        <div class="opinions_item__inner">
                            <img src="{{op.meta('grafika').url}}" class="img-fluid" alt="{{op.grafika.alt}}">
                            <p class="testimonial_text">{{op.opinia}}</p>
                            <p class="opinions_item__creator text-end"><b>{{op.autor}}</b> | {{op.stanowisko}}</p>
                            
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>

            <div class="d-none row opinions_pagination justify-content-center">
                <div class="col-12 col-lg-5">
                    <div class="opinions_pagination__prev"></div>
                    <ul class="d-flex justify-content-evenly">
                        <li></li>
                        <li></li>
                        <li></li>
                        <li class="active"></li>
                        <li></li>
                    </ul>
                    <div class="opinions_pagination__next"></div>
                </div>
            </div>
        </div>
    </div>
</section>

