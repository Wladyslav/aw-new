<section class="home_blog">
    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-5">
                <h2>
                    {{o.home_blog_h1}}
                </h2>
            </div>

            <div class="col-12 col-lg-7">
                <div class="row home_blog__right position-relative">
                    <div class="col-lg-10 g-lg-0 offset-lg-2">
                        <p>{{o.home_blog_desc}}</p>
                    </div>
                </div>
            </div>
        </div>

        <div class="home_blog-items row align-items-stretch">
            {% for b in posts_home %}
            <div class="col-12 col-lg-4">
                <div class="home_blog-item">
                    <div class="row flex-column h-100">
                        <div class="col-12">
                            <span class="home_blog-item__date">{{function('get_the_date', 'd.m.Y', b)}}</span>
                            <h3><a href="{{b.path}}">{{b.post_title}}</a></h3>
                            <p>{{ b.meta('short_desc') }}</p>
                        </div>
                        <div class="d-flex justify-content-end mt-auto">
                            <div class="home_blog-item__avatar">
                                <img src="{{ b.author.meta('profil_picture') }}" class="me-3" />
                            </div>
                            <div class="home_blog-item__person">
                                <span class="d-block">{{b.author.name}}</span>
                                <span>{{ b.author.meta('position') }}</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>

        <div class="col-12">
            <button class="btn btn-small hover d-block mx-auto"><a href="{{o.home_blog_button_link}}">{{o.home_blog_button}}</a></button>
        </div>
    </div>
</section>

<section class="home_form">
    <div class="container">
       

        <div class="row home_form__content">
            
            <div class="col-12 col-lg-6 order-1 order-md-0">
                <h2>
                    {{o.home_form_h1}}
                </h2>
                <h3 class="mt-5 mt-md-0">{{o.home_form_dane_kontaktowe}}</h3>
                <p class="mt-2">{{o.adres}}</p>
                <a href="{{o.home_form_wskazowki_link}}" class="with-arrow mt-3 home_form__tip d-block d-md-inline-block me-auto me-md-0">{{o.home_form_wskazowki}}</a>

                <p class="home_form__krs">
                    <span>KRS:</span> {{option.krs}}<br />
                    <span>NIP:</span> {{option.nip}}<br />
                    <span>E-mail:</span> {{option.email}}<br />
                    <span>Tel.</span> {{option.phone}}
                </p>

                <h3 class="mt-5 mb-4">Sprawd≈∫ nasze social media</h3>

                <div class="home_form__social mb-5 mb-lg-0">
                    <span class="home_form__icon facebook">/{{option.facebook}}</span>
                    <span class="home_form__icon twitter">/{{option.twitter}}</span>
                </div>
            </div>
            <div class="col-12 col-lg-6 g-0 position-relative">
                {% include "form.twig" %}
                <div class="home_form__box--shadow"></div>
                <div class="circle_1 d-none d-md-block"></div>
                <div class="circle_2 d-none d-md-block"></div>
                <div class="circle_3 d-block d-md-none"></div>
            </div>
        </div>
    </div>
</section>


