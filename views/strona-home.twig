<section class="home_clients_banner">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <span>{{o.home_clients_banner_h1}}</span>
                <div class="d-flex justify-content-between home_clients_banner_element">
                    {% for i in o.home_clients_banner_images %}
