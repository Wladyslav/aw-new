<section class="contact-section-wrapper">
    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-6 contact-data-wrapper ">
                <h3 class="contact-section-heading">Dane kontaktowe</h3>
                <div class="contact-data">
                    <p>{{option.adres}}</p>
                    <p>KRS: {{option.krs}}</p>
                    <p>NIP: {{option.nip}}</p>
                    <p>{{option.email}}</p>
                    <a href="tel:+48{{ option.phone|replace({' ': ''}) }}" class="contact-tel">{{ option.phone }}</a>
                </div>
            </div>
            <div class="col-12 col-lg-6 contact-map">
                <iframe src="{{section.map_link}}" width="100%" height="100%" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>

        </div>
    </div>


</section>